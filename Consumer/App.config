<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="eventConsumersSection" type="Consumer.CustomConfiguration.EventConsumersSection, Consumer" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <eventConsumersSection>
    <eventConsumers>
      <eventConsumer name="ApplicationEventConsumer" eventSource="Provider-Application-EventSource" applicationName="Provider-Application" rollingLogPath="C:\logs\Provider\" rollingLogFileName="application" logDebugTracesToDatabase="true" />
      <!--<eventConsumer name="BusEventConsumer"
                     eventSource="Provider-Bus-EventSource" applicationName="Provider-Bus"
                      rollingLogPath="C:\logs\Provider\" rollingLogFileName="bus" logDebugTracesToDatabase="false"/>-->
    </eventConsumers>
  </eventConsumersSection>
  <appSettings>
    <add key="ScomEventSource" value="OurScomEventSource" />
  </appSettings>
  <connectionStrings>
    <add name="TraceDatabase" connectionString="Server=.\sqlserver2014;Database=TraceDatabase;Trusted_Connection=True;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <runtime>
    <gcServer enabled="true" />
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="SimpleInjector" publicKeyToken="984cb50dea722e99" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.2.7.0" newVersion="3.2.7.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>